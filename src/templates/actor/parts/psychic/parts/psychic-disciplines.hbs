{{#> "systems/animabf/templates/actor/parts/common/ui/group.hbs"
  class="psychic-disciplines"
}}
  {{>
  "systems/animabf/templates/actor/parts/common/ui/group-header.hbs"
    title=(localize "anima.ui.psychic.psychicDiscipline.title")
    addRowButtonData="add-psychic-discipline"
  }}
  {{#> "systems/animabf/templates/actor/parts/common/ui/group-body.hbs"
    class="psychic-disciplines"
  }}
    {{#each data.data.psychic.psychicDisciplines}}
      <div data-psychic-discipline-id='{{this._id}}' class='psychic-discipline-row'>
        <input
          name='data.dynamic.psychicDisciplines.{{this._id}}.name'
          type='text'
          class='input'
          value='{{this.name}}'
        />
      </div>
    {{/each}}
  {{/"systems/animabf/templates/actor/parts/common/ui/group-body.hbs"}}
{{/"systems/animabf/templates/actor/parts/common/ui/group.hbs"}}