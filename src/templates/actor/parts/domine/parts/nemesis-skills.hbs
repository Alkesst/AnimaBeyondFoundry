{{#> "systems/animabf/templates/common/ui/group.hbs"
  class="nemesis-skills"
}}
  {{>
  "systems/animabf/templates/common/ui/group-header.hbs"
    title=(localize "anima.ui.domine.nemesisSkills.title")
    addRowButtonData="add-nemesis-skill"
  }}
  {{#> "systems/animabf/templates/common/ui/group-body.hbs"
    class="nemesis-skills"
  }}
     {{#each data.data.domine.nemesisSkills as |atr key|}}
        {{>
        "systems/animabf/templates/common/ui/horizontal-titled-input.hbs"
          class="nemesis-skill-row"
          title = (localize (concat "anima.ui.domine.nemesisSkills." key ".title"))
          inputType="checkbox"
          inputName=(concat "data.domine.nemesis." key ".isActive")
          inputValue=atr.isActive
        }}
    {{/each}}
  {{/"systems/animabf/templates/common/ui/group-body.hbs"}}
{{/"systems/animabf/templates/common/ui/group.hbs"}}