{{#> "systems/animabf/templates/common/ui/group.hbs"
  class="ki-skills"
  contractible=true
  contractibleItemId=data.data.domine.kiSkills
}}
  {{>
  "systems/animabf/templates/common/ui/group-header.hbs"
    title=(localize "anima.ui.domine.kiSkills.title")
  }}
  {{#> "systems/animabf/templates/common/ui/group-body.hbs"
    class="ki-skills"
  }}
    {{#each data.data.domine.kiSkills as |atr key|}}
        {{>
        "systems/animabf/templates/common/ui/horizontal-titled-input.hbs"
          class="ki-skill-row"
          title = (localize (concat "anima.ui.domine.kiSkills." key ".title"))
          inputType="checkbox"
          inputName=(concat "data.domine.kiSkills." key ".isActive")
          inputValue=atr.isActive
        }}
    {{/each}}
  {{/"systems/animabf/templates/common/ui/group-body.hbs"}}
{{/"systems/animabf/templates/common/ui/group.hbs"}}