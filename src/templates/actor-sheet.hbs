<form class="{{cssClass}} actor-sheet" autocomplete="off">
  <header class='header-fields'>
    {{> "systems/animabf/templates/parts/header/header.hbs"}}
  </header>

  {{!-- Character Sheet Navigation --}}
  <nav class="sheet-tabs tabs" data-group="primary">
    <a class="item" data-tab="main">{{localize "anima.main"}}</a>
    <a class="item" data-tab="secondaries">{{localize "anima.secondaries"}}</a>
    <a class="item" data-tab="combat">{{localize "anima.combat"}}</a>
    <a class="item" data-tab="mystic">{{localize "anima.mystic"}}</a>
    <a class="item" data-tab="domine">{{localize "anima.domine"}}</a>
    <a class="item" data-tab="psychic">{{localize "anima.psychics"}}</a>
  </nav>

  <section class="sheet-body">
    <div class="tab main" data-group="primary" data-tab="main">
      {{> "systems/animabf/templates/parts/general/general.hbs"}}
    </div>

    <div class="tab secondaries" data-group="primary" data-tab="secondaries">
      {{> "systems/animabf/templates/parts/secondaries.html"}}
    </div>

    <div class="tab combat" data-group="primary" data-tab="combat">
      {{> "systems/animabf/templates/parts/combat.html"}}
    </div>

    <div class="tab mystic" data-group="primary" data-tab="mystic">
      {{> "systems/animabf/templates/parts/mystic/mystic.hbs"}}
    </div>

    <div class="tab domine" data-group="primary" data-tab="domine">
      {{> "systems/animabf/templates/parts/domine.html"}}
    </div>

    <div class="tab psychics" data-group="primary" data-tab="psychic">
      {{> "systems/animabf/templates/parts/psychic/psychic.hbs"}}
    </div>
  </section>
</form>
